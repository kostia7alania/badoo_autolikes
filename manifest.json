{
    "manifest_version": 2,
    "name": "badoo_autolike",
    "version": "1.0",
    "icons":{
        "128":"icon_activ.jpg"
    },
    "content_scripts":[{
        "matches": ["*://*/*"],
        "js": ["vue.min.js", "content.js"]
    }],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action": {
        "default_title": "NY",
        "default_icon":"icon.png",
        "default_popup":"index.html"
    },

    "web_accessible_resources":[],

    "permissions":["https://badoo.com/*"]

}